<UserControl x:Class="WpUI.LeftMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpUI"
             mc:Ignorable="d" 
             d:DesignHeight="570.4" d:DesignWidth="200">
    <Grid>
        <StackPanel>
            <Label Content="Algorithm List" Foreground="White" FontWeight="Bold" FontSize="18" HorizontalAlignment="Center" HorizontalContentAlignment="Center" />
            <ListBox HorizontalAlignment="Left" Height="570.4" VerticalAlignment="Top" Width="200" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}">
                <ListBoxItem Foreground="White" FontSize="14" FontWeight="Bold" Height="45">Item1</ListBoxItem>
                <ListBoxItem Foreground="White" FontSize="14" FontWeight="Bold" Height="45">Item2</ListBoxItem>
                <ListBoxItem Foreground="White" FontSize="14" FontWeight="Bold" Height="45">Item3</ListBoxItem>
                <ListBoxItem Foreground="White" FontSize="14" FontWeight="Bold" Height="45">Item4</ListBoxItem>
                <ListBoxItem Foreground="White" FontSize="14" FontWeight="Bold" Height="45">Item5</ListBoxItem>
            </ListBox>
        </StackPanel>

    </Grid>
</UserControl>
